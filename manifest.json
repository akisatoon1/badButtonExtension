{
    "manifest_version": 3,
    "name": "自動低評価マシーン",
    "version": "1.0.0",
    "description": "YouTube上のあらゆる動画に対し爆速で低評価を付けます。",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "author": {
        "email": "akisatoon@gmail.com"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ]
}